{
  "version": 2,
  "functions": {
    "api/contact.js": {
      "maxDuration": 10
    },
    "api/newsletter.js": {
      "maxDuration": 10
    },
    "api/roadmap.js": {
      "maxDuration": 10
    },
    "api/health.js": {
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
