{
  "name": "aris-ai-data-analyst",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "npm run build && node analyze-bundle.js",
    "build:prod": "NODE_ENV=production npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:prod": "npm run build:prod && npm run preview",
    "backend": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend\"",
    "build:full": "npm run build && npm run backend:build",
    "start:full": "concurrently \"npm run preview\" \"npm run backend:start\"",
    "analyze": "node analyze-bundle.js",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@types/d3": "^7.4.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/three": "^0.179.0",
    "axios": "^1.6.2",
    "d3": "^7.9.0",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.1.2",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
